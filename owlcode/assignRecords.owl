program 'assignRecords';
begin 
    record node begin
        let info;
        let next;
    end
    let z := make node;
    let x := make node;
    let y := make node;
    let it := 2;
    x.info := 13;
    y.info := 42;
    x.next := y;
    z.next := 666;
    y.next := z;
    z.info := 666;
    it := x;
    while ((it.info) != 666) begin 
        print it.info;
        print " ";
        it := it.next;
    end;
    print "\n";
    if ((it.info) == z.info) then 
        print "dat fooseball\n";
    end;
end